<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Dispensers\Domain\Model\Dispenser" table="dispensers">
        <id name="id" type="dispenser_id" column="id" length="36" />

        <embedded name="flow_volume" class="App\Dispensers\Domain\Model\ValueObject\DispenserFlowVolume" use-column-prefix="false" />
        <embedded name="status" class="App\Dispensers\Domain\Model\ValueObject\DispenserStatus" use-column-prefix="false" />
        <embedded name="created_on" class="App\Dispensers\Domain\Model\ValueObject\DispenserCreatedOn" use-column-prefix="false" />
        <embedded name="updated_on" class="App\Dispensers\Domain\Model\ValueObject\DispenserUpdatedOn" use-column-prefix="false" />
    </entity>

</doctrine-mapping>
